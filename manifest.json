{
  "name": "Getting Started Example",
  "version": "0.1",
  "description": "Build an Extension!",
  "permissions": ["tabs"],
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": { 
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
   "128": "icons/icon128.png" 
  },
  "content_security_policy": "script-src 'self' https://gstatic.com https://www.googletagmanager.com; object-src 'self'",
  "manifest_version": 2
}